  <meta content="Bulk editing of page properties allows you to edit the properties of multiple pages at once" name="seoDescription" /> 
  <meta content="products:SG_EXPERIENCEMANAGER/6.4/SITES;topic_tags:extending-aem;content_type:reference" name="cq:tags" /> 
